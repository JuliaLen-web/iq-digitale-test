include ../../mixins/projects-slide

.projects.align-section
  .container
    .decor__square.decor__square_accent
    .decor__circle
    .projects__content(
      data-aos="fade-up"
      data-aos-easing="ease-out-cubic"
      data-aos-duration="500"
    )
      .title Our recent #[span projects]
      span.projects__content-line
      .projects__content-subtitle Website development for Digital Company
      p My first thought about art, as a child, was that the artist brings something into the world that didn't exist before, and that he does it without destroying something else
    .projects__gallery.swiper(
      data-aos="fade-up"
      data-aos-easing="ease-out-cubic"
      data-aos-duration="600"
      data-aos-delay="150"
    )
      .swiper-wrapper
        +projects-slide({
          photoOne: require('@/assets/img/cat.jpg'),
          photoTwo: require('@/assets/img/flowers.jpg') ,
          photoThree: require('@/assets/img/music.jpg')
        })
        +projects-slide({
          photoOne: require('@/assets/img/grass.jpg'),
          photoTwo: require('@/assets/img/cat.jpg'),
          photoThree: require('@/assets/img/people.jpg')
        })
        +projects-slide({
          photoOne: require('@/assets/img/music.jpg'),
          photoTwo: require('@/assets/img/flowers.jpg'),
          photoThree: require('@/assets/img/cat.jpg')
        })
        +projects-slide({
          photoOne: require('@/assets/img/music.jpg'),
          photoTwo: require('@/assets/img/cat.jpg'),
          photoThree: require('@/assets/img/grass.jpg')
        })
        +projects-slide({
          photoOne: require('@/assets/img/cat.jpg'),
          photoTwo: require('@/assets/img/person_3.jpg'),
          photoThree: require('@/assets/img/music.jpg')
        })
      .swiper__process
